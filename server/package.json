{
  "name": "nestjs-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@mikro-orm/core": "^5.3.1",
    "@mikro-orm/mongo-highlighter": "^1.0.0",
    "@mikro-orm/mongodb": "^5.3.1",
    "@mikro-orm/nestjs": "^5.1.0",
    "@mikro-orm/reflection": "^5.3.1",
    "@nestjs/common": "^9.0.8",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.8",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.8",
    "@nestjs/platform-socket.io": "^9.0.9",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/websockets": "^9.0.9",
    "@socket.io/redis-adapter": "^7.2.0",
    "aws-sdk": "^2.1190.0",
    "bcrypt": "^5.0.1",
    "bson": "^4.6.5",
    "bson-objectid": "^2.0.3",
    "cache-manager": "^4.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.2",
    "image-size": "^1.0.2",
    "lodash": "^4.17.21",
    "mediasoup": "^3.10.8",
    "moment": "^2.29.4",
    "open-graph-scraper": "^4.11.1",
    "passport-jwt": "^4.0.0",
    "prettier": "^2.7.1",
    "probe-image-size": "^7.2.3",
    "randomstring": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "seedrandom": "^3.0.5",
    "sharp": "^0.30.7",
    "short-uuid": "^4.2.0",
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.3.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.8",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.6.5",
    "@types/open-graph-scraper": "^4.8.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/probe-image-size": "^7.2.0",
    "@types/randomstring": "^1.1.8",
    "@types/seedrandom": "^3.0.2",
    "@types/sharp": "^0.30.5",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "28.1.3",
    "pino-pretty": "^8.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/a(|/.*)$": "<rootDir>/libs/a/src/$1"
    }
  }
}
